{
    "name": "angular",
    "version": "0.0.1",
    "scripts": {
        "clean": "rm -rf ClientApp/dist && rm -rf wwwroot/dist && rm -rf ClientApp/test/coverage",
        "webpack:prod:polyfills": "webpack --config webpack.config.polyfills.js --env.prod",
        "webpack:prod:vendor": "webpack --config webpack.config.vendor.js --env.prod",
        "webpack:prod": "webpack --env.prod",
        "build:prod": "npm run clean && npm run webpack:prod:polyfills && npm run webpack:prod:vendor && npm run webpack:prod",
        "webpack:polyfills": "webpack --config webpack.config.polyfills.js",
        "webpack:vendor": "webpack --config webpack.config.vendor.js",
        "build": "npm run clean && npm run webpack:polyfills && npm run webpack:vendor && webpack",
        "test": "jest",
        "test:ci": "npm run test -- --runInBand",
        "test:coverage": "npm run test -- --coverage && http-server -c-1 -o -p 9875 ClientApp/test/coverage/lcov-report",
        "test:watch": "npm run test -- --watch -- please-enter-name-of-filter"
    },
    "jest": {
        "coverageDirectory": "ClientApp/test/coverage",
        "coveragePathIgnorePatterns": [
            "/node_modules/",
            "<rootDir>/ClientApp/test/.*.ts"
        ],
        "globals": {
            "__TRANSFORM_HTML__": true,
            "ts-jest": {
                "tsConfigFile": "ClientApp/tsconfig.spec.json"
            }
        },
        "preset": "jest-preset-angular",
        "setupTestFrameworkScriptFile": "./ClientApp/test/setupJest.ts"
    },
    "dependencies": {
        "@angular/animations": "^4.3.4",
        "@angular/common": "^4.3.4",
        "@angular/compiler": "^4.3.4",
        "@angular/compiler-cli": "^4.3.4",
        "@angular/core": "^4.3.4",
        "@angular/forms": "^4.3.4",
        "@angular/http": "^4.3.4",
        "@angular/platform-browser": "^4.3.4",
        "@angular/platform-browser-dynamic": "^4.3.4",
        "@angular/platform-server": "^4.3.4",
        "@angular/router": "^4.3.4",
        "@types/node": "8.0.8",
        "aspnet-prerendering": "^2.0.5",
        "preboot": "4.5.2",
        "rxjs": "^5.4.2",
        "typescript": "^2.4.1",
        "zone.js": "0.8.12",


        "blob.js": "^1.0.1",
        "es6-shim": "^0.35.3",
        "event-source-polyfill": "^0.0.9",
        "formdata-polyfill": "^2.0.3",
        "core-js": "^2.4.1",
        "classlist.js": "^1.1.20150312",
        "intl": "^1.2.5",
        "isomorphic-fetch": "^2.2.1",
        "js-polyfills": "^0.1.33",
        "reflect-metadata": "^0.1.10",
        "web-animations-js": "^2.3.1",


        "@ng-bootstrap/ng-bootstrap": "^1.0.0-beta",
        "bootstrap": "^4.0.0-beta",
        "font-awesome": "^4.7.0",
        "jquery": "3.2.1",
        "ng2-page-scroll": "^4.0.0-beta.11",
        "ng2-slim-loading-bar": "^4.0.0",
        "popper.js": "^1.12.3",


        "angular2-template-loader": "0.6.2",
        "awesome-typescript-loader": "3.1.2",
        "css": "2.2.1",
        "css-loader": "0.28.0",
        "expose-loader": "0.7.3",
        "file-loader": "0.11.1",
        "html-loader": "0.4.5",
        "json-loader": "0.5.4",
        "null-loader": "^0.1.1",
        "postcss-loader": "^2.0.6",
        "raw-loader": "0.5.1",
        "style-loader": "0.16.1",
        "to-string-loader": "1.1.5",
        "url-loader": "0.5.8",


        "rimraf": "^2.6.1",
        "@ngtools/webpack": "^1.6.0",
        "aspnet-webpack": "^2.0.1",
        "extract-text-webpack-plugin": "^2.1.2",
        "uglifyjs-webpack-plugin": "^0.4.6",
        "webpack": "^2.7.0",
        "webpack-hot-middleware": "^2.18.2",
        "webpack-merge": "^4.1.0"
    },
    "devDependencies": {
        "@types/chai": "^3.4.34",
        "@types/jasmine": "^2.5.37",
        "@types/jest": "^19.2.3",
        "chai": "^3.5.0",
        "http-server": "^0.10.0",
        "jasmine-core": "^2.5.2",
        "jest": "^20.0.0",
        "jest-preset-angular": "^2.0.1"
    }
}