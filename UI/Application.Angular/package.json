{
  "name": "angular.net",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "build": "npm run webpack:polyfills && npm run webpack:vendor && npm run webpack",
    "build:prod": "npm run webpack:polyfills:prod && npm run webpack:vendor:prod && npm run webpack:prod",
    "clean": "rm -rf ClientApp/dist && rm -rf wwwroot/dist && rm -rf ClientApp/test/coverage",
    "test": "jest",
    "test:ci": "npm run test -- --runInBand",
    "test:coverage": "npm run test -- --coverage && http-server -c-1 -o -p 9875 ClientApp/test/coverage/lcov-report",
    "test:watch": "npm run test -- --watch -- please-enter-name-of-filter",
    "webpack": "webpack --progress --color",
    "webpack:polyfills": "webpack --config webpack.config.polyfills.js --progress --color",
    "webpack:polyfills:prod": "webpack --config webpack.config.polyfills.js --env.prod --progress --color",
    "webpack:prod": "webpack --env.prod --progress --color",
    "webpack:vendor": "webpack --config webpack.config.vendor.js --progress --color",
    "webpack:vendor:prod": "webpack --config webpack.config.vendor.js --env.prod --progress --color"
  },
  "jest": {
    "coverageDirectory": "ClientApp/test/coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/ClientApp/test/",
      "<rootDir>/ClientApp/app/mock.models/",
      "<rootDir>/ClientApp/app/mock.services/",
      "<rootDir>/ClientApp/modules/"
    ],
    "globals": {
      "__TRANSFORM_HTML__": true,
      "ts-jest": {
        "tsConfigFile": "ClientApp/tsconfig.spec.json"
      }
    },
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "./ClientApp/test/setupJest.ts",
    "testResultsProcessor": "jest-junit"
  },
  "jest-junit": {
    "ancestorSeparator": " | ",
    "classNameTemplate": "{classname}-{title}",
    "output": "./TEST-junit.xml",
    "suiteName": "UI tests",
    "titleTemplate": "{classname}-{title}",
    "usePathForSuiteName": "true"
  },
  "dependencies": {
    "@angular/animations": "^4.4.4",
    "@angular/common": "^4.4.4",
    "@angular/compiler": "^4.4.4",
    "@angular/compiler-cli": "^4.4.4",
    "@angular/core": "^4.4.4",
    "@angular/forms": "^4.4.4",
    "@angular/http": "^4.4.4",
    "@angular/platform-browser": "^4.4.4",
    "@angular/platform-browser-dynamic": "^4.4.4",
    "@angular/platform-server": "^4.4.4",
    "@angular/router": "^4.4.4",
    "@ngtools/webpack": "^1.9.3",
    "@types/node": "^9.3.0",
    "angular2-template-loader": "^0.6.2",
    "aspnet-prerendering": "^3.0.1",
    "aspnet-webpack": "^2.0.1",
    "awesome-typescript-loader": "^3.4.1",
    "blob.js": "^1.0.1",
    "bootstrap": "^4.0.0-beta",
    "classlist.js": "^1.1.20150312",
    "core-js": "~2.5.3",
    "css": "^2.2.1",
    "css-loader": "^0.28.9",
    "es6-shim": "^0.35.3",
    "event-source-polyfill": "^0.0.12",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "formdata-polyfill": "^3.0.9",
    "fsync": "^0.0.0",
    "html-loader": "^0.5.5",
    "intl": "^1.2.5",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.2.1",
    "js-polyfills": "^0.1.41",
    "ng2-slim-loading-bar": "^4.0.0",
    "null-loader": "^0.1.1",
    "popper.js": "^1.12.9",
    "postcss-loader": "^2.0.10",
    "preboot": "^5.0.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rmdir": "^1.2.0",
    "rxjs": "^5.5.6",
    "scandir": "^0.1.2",
    "style-loader": "^0.19.1",
    "to-string-loader": "^1.1.5",
    "typescript": "2.5.3",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "url-loader": "^0.6.2",
    "web-animations-js": "^2.3.1",
    "webpack": "^3.10.0",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-merge": "^4.1.1",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
    "@types/chai": "^4.1.1",
    "@types/jasmine": "^2.8.4",
    "@types/jest": "^20.0.0",
    "chai": "^4.1.2",
    "http-server": "^0.11.1",
    "jasmine-core": "^2.9.0",
    "jest": "^20.0.0",
    "jest-junit": "^3.4.1",
    "jest-preset-angular": "~3.0.0"
  }
}
